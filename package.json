{
  "name": "express-framework",
  "version": "1.0.0",
  "description": "Express.js with TypeScript decorators",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch \"src/**/*.ts\" --exec \"npx ts-node src/index.ts\"",
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "format": "prettier --write \"src/**/*.{js,ts,json,md}\"",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "keywords": [
    "express",
    "typescript",
    "decorators",
    "api"
  ],
  "author": "liuyang",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.4",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.1",
    "pg-connection-string": "^2.9.1",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.1.14",
    "typeorm": "^0.3.24",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^18.15.11",
    "@types/redis": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4"
  }
}
